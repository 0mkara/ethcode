{
  "name": "ethcode",
  "displayName": "ETHcode",
  "description": "Ethereum IDE for VS Code",
  "version": "0.0.0",
  "private": true,
  "publisher": "0mkara",
  "repository": "https://github.com/0mkara/ethcode",
  "engines": {
    "vscode": "^1.25.0"
  },
  "activationEvents": [
    "onCommand:ethcode.activate",
    "onCommand:ethcode.compile",
    "onCommand:ethcode.refactor"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ethcode.activate",
        "title": "Activate ETHcode extension",
        "category": "Ethereum"
      },
      {
        "command": "ethcode.compile",
        "title": "Solidity compile",
        "category": "Ethereum"
      },
      {
        "command": "ethcode.refactor",
        "title": "Solidity compile refactor",
        "category": "Ethereum"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+alt+e",
        "command": "ethcode.activate"
      },
      {
        "key": "ctrl+alt+c",
        "command": "ethcode.compile"
      }
    ]
  },
  "dependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "12.0.2",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts": "3.0.1",
    "react-select": "^2.4.3",
    "redux": "^4.0.1",
    "remix-url-resolver": "^0.0.5",
    "typescript": "3.4.5",
    "vscode": "^1.1.34"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && tsc -p tsconfig.extension.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch": "tsc -watch -p ./"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {}
}
