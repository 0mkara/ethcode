{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
	    "ext-src/**/*.ts"
    ]
  },
  "rules": {
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "jsx-no-lambda": false
  }
}
